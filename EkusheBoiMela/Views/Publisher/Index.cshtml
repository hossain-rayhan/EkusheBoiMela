@model IEnumerable<EkusheBoiMela.Model.Entity.Publisher>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameInEnglish)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StallNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StallLat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StallLong)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreationDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameInEnglish)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StallNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StallLat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StallLong)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreationDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
